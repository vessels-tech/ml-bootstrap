{
  "name": "@mojaloop/oss-lab-bootstrap",
  "version": "0.1.0",
  "description": "Bootstrapper tool for Mojaloop OSS Lab",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint ./src/**/*.ts *.js",
    "build": "tsc",
    "watch": "tsc -w",
    "reseed:docker-live": "echo \"seeding docker-live environment\" && ./src/seed/cli.ts",
    "test:coverage": "jest --coverage --coverageThreshold='{}'",
    "test:coverage-check": "jest --coverage",
    "test:junit": "jest --reporters=default --reporters=jest-junit",
    "test:unit": "jest --testMatch '**/test/unit/**/*.(test|spec).ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojaloop/hackathon-docs.git"
  },
  "keywords": [
    "metrics",
    "central",
    "services"
  ],
  "author": "lewisdaly",
  "contributors": [
    "Lewis Daly <lewis@vesselstech.com>"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@mojaloop/sdk-standard-components": "^12.0.1",
    "axios": "^0.19.2",
    "chalk": "^4.1.0"
  },
  "devDependencies": {
    "@mojaloop/api-snippets": "11.0.18",
    "@types/jest": "25.2.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-cucumber": "^1.4.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "got": "^11.8.0",
    "husky": "^4.2.5",
    "jest": "26.0.1",
    "jest-cucumber": "^2.0.11",
    "jest-junit": "10.0.0",
    "lint-staged": "^10.2.3",
    "newman": "^5.0.1",
    "node-plantuml": "^0.9.0",
    "plantuml-encoder": "^1.4.0",
    "source-map-support": "0.5.12",
    "svgo": "^1.3.2",
    "swagger-cli": "^4.0.3",
    "ts-jest": "^26.0.0",
    "ts-node": "8.3.0",
    "typescript": "3.9.2",
    "uuid": "^8.3.1"
  }
}
